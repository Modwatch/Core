{
  "name": "modwatch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "watch:js": "rollup -c --watch",
    "pack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "pack:watch": "webpack --watch --config webpack.config.js",
    "build:css": "cross-env NODE_ENV=production postcss --config postcss.config.js --map style.css.map -o ./public/dist/styles.css ./src/css/index.css",
    "watch:css": "postcss --config postcss.config.js --map style.css.map -lw -o ./public/dist/styles.css ./src/css/index.css",
    "serve": "firebase serve --port 3000 --host 0.0.0.0"
  },
  "main": "server.js",
  "dependencies": {
    "es6-promise": "^4.1.1",
    "jwt-decode": "^2.2.0",
    "marked": "^0.3.6",
    "snarkdown": "^1.2.2",
    "unfetch": "^2.1.2",
    "vue": "2.3.3",
    "vue-router": "2.5.3",
    "vuex": "2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.2",
    "babel-preset-env": "^1.5.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.3.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "postcss": "^5.0.17",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nesting": "^4.0.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0"
  },
  "subdomain": "skyrimmodwatcher",
  "domains": [
    "modwat.ch",
    "www.modwat.ch"
  ],
  "engines": {
    "node": "^0.10.0"
  }
}
