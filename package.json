{
  "name": "modwatch",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build:node": "babel ./src/node --out-dir ./server",
    "cli": "node scripts/cli.js",
    "build:cli": "browserify --no-bundle-external --node ./src/scripts/interfaces/cli.js -t [babelify --presets es2015] -o ./scripts/cli.js",
    "build": "browserify ./src/browserify.entry.js -t [babelify] -t [partialify] -p [css-modulesify -u cssnano -o ./public/dist/styles.css] -o ./public/dist/bundle.dev.js -dv",
    "watch": "watchify ./src/browserify.entry.js -t [babelify] -t [partialify] -p [css-modulesify -u cssnano -o ./public/dist/styles.css] -o ./public/dist/bundle.dev.js -dv",
    "deploy": "browserify ./src/browserify.entry.js -t [babelify] -t [partialify] -p [css-modulesify -u cssnano -o ./public/dist/styles.prod.css] | uglifyjs -mc --screw-ie8 > ./public/dist/bundle.prod.js",
    "prepublish": "npm run build:node && npm run build:cli"
  },
  "main": "server.js",
  "dependencies": {
    "@ansballard/paginate": "^1.1.4",
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-local-storage": "^0.2.2",
    "angular-route": "^1.5.8",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.12.0",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.3.4",
    "ejs": "1.0.x",
    "express": "^4.11.2",
    "express-session": "^1.10.3",
    "method-override": "^2.3.1",
    "request": "^2.60.0",
    "serve-favicon": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chokidar": "^1.5.1",
    "concurrently": "^2.0.0",
    "core-js": "^2.3.0",
    "cssnano": "^3.5.2",
    "denodeify": "^1.2.1",
    "glob": "^7.0.0",
    "highlight.js": "^9.2.0",
    "html-minifier": "^1.2.0",
    "http-server": "^0.9.0",
    "keypress": "^0.2.1",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "ora": "^0.2.1",
    "partialify": "^3.1.6",
    "postcss": "^5.0.17",
    "postcss-cli": "^2.5.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "prompt": "^1.0.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-string": "^2.0.2",
    "rollupify": "^0.1.0",
    "uglify-js": "^2.6.2",
    "uncss": "^0.13.0"
  },
  "subdomain": "skyrimmodwatcher",
  "domains": [
    "modwat.ch",
    "www.modwat.ch"
  ],
  "engines": {
    "node": "^0.10.0"
  }
}
