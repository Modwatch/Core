{
  "name": "modwatch",
  "version": "2.0.8",
  "private": true,
  "scripts": {
    "build": "cross-env API_URL=http://api.modwat.ch NODE_ENV=production parcel build -d public/dist -o bundle.js src/vue/index.jsx",
    "watch": "cross-env API_URL=http://localhost:3001 NODE_ENV=development parcel watch -d public/dist -o bundle.js src/vue/index.jsx",
    "serve": "serve public -l tcp://0.0.0.0:3000",
    "prettier": "prettier --write 'src/**/*.{js,jsx,css}'",
    "deploy:ci": "firebase use testing-ee4f4 --token $FIREBASE_TOKEN && firebase deploy --non-interactive --token $FIREBASE_TOKEN"
  },
  "precommit": [
    "prettier",
    "build"
  ],
  "main": "server.js",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "es6-promise": "^4.2.5",
    "jwt-decode": "^2.2.0",
    "postcss-preset-env": "^6.0.7",
    "preact": "^8.3.1",
    "preact-router": "^2.6.1",
    "promiscuous": "^0.7.2",
    "unfetch": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^3.3.1",
    "firebase-tools": "^3.19.3",
    "parcel-bundler": "^1.10.1",
    "postcss-import": "^10.0.0",
    "postcss-nesting": "^4.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "serve": "^10.0.2"
  },
  "subdomain": "skyrimmodwatcher",
  "domains": [
    "modwat.ch",
    "www.modwat.ch"
  ]
}
